{
    "name": "@byters/intern",
    "description": " ",
    "version": "0.0.1",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "private": false,
    "author": {
        "name": "Byters",
        "url": "https://github.com/BytersProject/"
    },
    "maintainer": [
        {
            "email": "yo@quantumlytangled.com",
            "name": "QuantumlyTangled",
